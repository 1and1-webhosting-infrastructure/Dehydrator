<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>Dehydrator.WebApi</id>
		<version>$version$</version>
		<authors>Bastian Eicher</authors>
		<copyright>1&amp;1 Internet SE</copyright>
